# VitalFlow - Streamlit Secrets Configuration
# Copy this file to secrets.toml and fill in your values
# For Streamlit Cloud deployment, add these in the app settings

# Google OAuth 2.0 Credentials
# Get these from Google Cloud Console: https://console.cloud.google.com
GOOGLE_CLIENT_ID = "your-client-id.apps.googleusercontent.com"
GOOGLE_CLIENT_SECRET = "your-client-secret"

# Redirect URI - Set this to your deployed app URL
# For Streamlit Cloud: https://your-app-name.streamlit.app/
# For local development: http://localhost:8502/
GOOGLE_REDIRECT_URI = "https://your-app-name.streamlit.app/"

# Optional: App Configuration
APP_NAME = "VitalFlow AI"
APP_ENV = "production"  # "development" or "production"
